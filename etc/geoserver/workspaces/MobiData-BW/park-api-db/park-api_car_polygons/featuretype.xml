    <entry key="JDBC_VIRTUAL_TABLE">
      <virtualTable>
        <name>park-api_car_polygons</name>
        <sql>SELECT&#xd;
    id,&#xd;
    NULL AS parking_site_id,&#xd;
    source_id,&#xd;
    original_uid,&#xd;
    name,&#xd;
    operator_name,&#xd;
    purpose,&#xd;
    type :: text,&#xd;
    &apos;site&apos; as parking_object,&#xd;
    address,&#xd;
    has_realtime_data,&#xd;
    NULL AS realtime_status,&#xd;
    capacity,&#xd;
    NULL AS restriction_type,&#xd;
    NULL AS restriction_hours,&#xd;
    NULL AS restriction_max_stay,&#xd;
    side,&#xd;
    orientation,&#xd;
    parking_type,&#xd;
    created_at,&#xd;
    modified_at,&#xd;
    static_data_updated_at,&#xd;
    ST_SetSRID(ST_GeomFromGeoJSON(geojson), 4326) AS geometry&#xd;
FROM&#xd;
    parking_site&#xd;
WHERE&#xd;
    purpose = &apos;CAR&apos;&#xd;
    AND duplicate_of_parking_site_id IS NULL&#xd;
    AND GeometryType(ST_SetSRID(ST_GeomFromGeoJSON(geojson), 4326)) = &apos;POLYGON&apos;&#xd;
UNION&#xd;
SELECT&#xd;
    ps.id,&#xd;
    ps.parking_site_id,&#xd;
    ps.source_id,&#xd;
    ps.original_uid,&#xd;
    ps.name,&#xd;
    NULL AS operator_name,&#xd;
    ps.purpose :: text :: purposetype,&#xd;
    ps.type :: text AS type,&#xd;
    &apos;spot&apos; AS parking_object,&#xd;
    NULL AS address,&#xd;
    ps.has_realtime_data,&#xd;
    ps.realtime_status,&#xd;
    NULL AS capacity,&#xd;
    pr.type AS restriction_type,&#xd;
    pr.hours AS restriction_hours,&#xd;
    pr.max_stay AS restriction_max_stay,&#xd;
    NULL AS side,&#xd;
    NULL AS orientation,&#xd;
    NULL AS parking_type,&#xd;
    ps.created_at,&#xd;
    ps.modified_at,&#xd;
    ps.static_data_updated_at,&#xd;
    ST_SetSRID(ST_GeomFromGeoJSON(geojson), 4326) AS geometry&#xd;
FROM&#xd;
    parking_spot ps&#xd;
    LEFT JOIN parking_restriction pr ON ps.id = pr.parking_spot_id&#xd;
WHERE&#xd;
    purpose = &apos;CAR&apos;&#xd;
    AND GeometryType(ST_SetSRID(ST_GeomFromGeoJSON(geojson), 4326)) = &apos;POLYGON&apos;
</sql>

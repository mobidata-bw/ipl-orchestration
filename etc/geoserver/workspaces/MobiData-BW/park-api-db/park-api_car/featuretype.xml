        <sql>SELECT&#xd;
    -- Unique ID for GeoServer (avoids collisions between site and spot ids)&#xd;
    CONCAT(&apos;site-&apos;, site.id) AS uid,&#xd;
    site.id,&#xd;
    NULL AS parking_site_id,&#xd;
    source_id,&#xd;
    original_uid,&#xd;
    name,&#xd;
    operator_name,&#xd;
    purpose,&#xd;
    &apos;site&apos; as parking_object,&#xd;
    public_url,&#xd;
    photo_url,&#xd;
    address,&#xd;
    opening_hours,&#xd;
    description,&#xd;
    has_lighting,&#xd;
    fee_description,&#xd;
    has_fee,&#xd;
    park_and_ride_type,&#xd;
    has_realtime_data,&#xd;
    realtime_opening_status,&#xd;
    NULL AS realtime_status,&#xd;
    site.type :: text,&#xd;
    site.capacity,&#xd;
    site.realtime_capacity,&#xd;
    site.realtime_free_capacity,&#xd;
    pr_max_stay.max_stay AS max_stay,&#xd;
    CASE&#xd;
        WHEN pr_disabled.parking_site_id IS NULL THEN NULL&#xd;
        ELSE COALESCE(pr_disabled.capacity, site.capacity)&#xd;
    END AS capacity_disabled,&#xd;
    pr_disabled.realtime_capacity AS realtime_capacity_disabled,&#xd;
    pr_disabled.realtime_free_capacity AS realtime_free_capacity_disabled,&#xd;
    CASE&#xd;
        WHEN pr_women.parking_site_id IS NULL THEN NULL&#xd;
        ELSE COALESCE(pr_women.capacity, site.capacity)&#xd;
    END AS capacity_women,&#xd;
    pr_women.realtime_capacity AS realtime_capacity_women,&#xd;
    pr_women.realtime_free_capacity AS realtime_free_capacity_women,&#xd;
    CASE&#xd;
        WHEN pr_family.parking_site_id IS NULL THEN NULL&#xd;
        ELSE COALESCE(pr_family.capacity, site.capacity)&#xd;
    END AS capacity_family,&#xd;
    pr_family.realtime_capacity AS realtime_capacity_family,&#xd;
    pr_family.realtime_free_capacity AS realtime_free_capacity_family,&#xd;
    CASE&#xd;
        WHEN pr_charging.parking_site_id IS NULL THEN NULL&#xd;
        ELSE COALESCE(pr_charging.capacity, site.capacity)&#xd;
    END AS capacity_charging,&#xd;
    pr_charging.realtime_capacity AS realtime_capacity_charging,&#xd;
    pr_charging.realtime_free_capacity AS realtime_free_capacity_charging,&#xd;
    CASE&#xd;
        WHEN pr_carsharing.parking_site_id IS NULL THEN NULL&#xd;
        ELSE COALESCE(pr_carsharing.capacity, site.capacity)&#xd;
    END AS capacity_carsharing,&#xd;
    pr_carsharing.realtime_capacity AS realtime_capacity_carsharing,&#xd;
    pr_carsharing.realtime_free_capacity AS realtime_free_capacity_carsharing,&#xd;
    CASE&#xd;
        WHEN pr_resident.parking_site_id IS NULL THEN NULL&#xd;
        ELSE COALESCE(pr_resident.capacity, site.capacity)&#xd;
    END AS capacity_resident,&#xd;
    pr_resident.realtime_capacity AS realtime_capacity_resident,&#xd;
    pr_resident.realtime_free_capacity AS realtime_free_capacity_resident,&#xd;
    CASE&#xd;
        WHEN pr_bus.parking_site_id IS NULL THEN NULL&#xd;
        ELSE COALESCE(pr_bus.capacity, site.capacity)&#xd;
    END AS capacity_bus,&#xd;
    pr_bus.realtime_capacity AS realtime_capacity_bus,&#xd;
    pr_bus.realtime_free_capacity AS realtime_free_capacity_bus,&#xd;
    CASE&#xd;
        WHEN pr_caravan.parking_site_id IS NULL THEN NULL&#xd;
        ELSE COALESCE(pr_caravan.capacity, site.capacity)&#xd;
    END AS capacity_caravan,&#xd;
    pr_caravan.realtime_capacity AS realtime_capacity_caravan,&#xd;
    pr_caravan.realtime_free_capacity AS realtime_free_capacity_caravan,&#xd;
    max_height,&#xd;
    is_covered,&#xd;
    supervision_type,&#xd;
    related_location,&#xd;
    (&#xd;
        site.realtime_data_updated_at &lt; now() - interval &apos;30 minutes&apos;&#xd;
    ) AS realtime_data_outdated,&#xd;
    side,&#xd;
    orientation,&#xd;
    parking_type,&#xd;
    site.created_at,&#xd;
    site.modified_at,&#xd;
    static_data_updated_at,&#xd;
    realtime_data_updated_at,&#xd;
    GeometryType(ST_SetSRID(ST_GeomFromGeoJSON(geojson), 4326)) AS geojson,&#xd;
    geometry&#xd;
FROM&#xd;
    parking_site site&#xd;
    LEFT JOIN parking_restriction pr_max_stay ON pr_max_stay.parking_site_id = site.id&#xd;
    AND pr_max_stay.type IS NULL&#xd;
    AND pr_max_stay.max_stay IS NOT NULL&#xd;
    LEFT JOIN parking_restriction pr_disabled ON pr_disabled.parking_site_id = site.id&#xd;
    AND pr_disabled.type = &apos;DISABLED&apos;&#xd;
    LEFT JOIN parking_restriction pr_women ON pr_women.parking_site_id = site.id&#xd;
    AND pr_women.type = &apos;WOMEN&apos;&#xd;
    LEFT JOIN parking_restriction pr_family ON pr_family.parking_site_id = site.id&#xd;
    AND pr_family.type = &apos;FAMILY&apos;&#xd;
    LEFT JOIN parking_restriction pr_charging ON pr_charging.parking_site_id = site.id&#xd;
    AND pr_charging.type = &apos;CHARGING&apos;&#xd;
    LEFT JOIN parking_restriction pr_carsharing ON pr_carsharing.parking_site_id = site.id&#xd;
    AND pr_carsharing.type = &apos;CARSHARING&apos;&#xd;
    LEFT JOIN parking_restriction pr_resident ON pr_resident.parking_site_id = site.id&#xd;
    AND pr_resident.type = &apos;RESIDENT&apos;&#xd;
    LEFT JOIN parking_restriction pr_bus ON pr_bus.parking_site_id = site.id&#xd;
    AND pr_bus.type = &apos;BUS&apos;&#xd;
    LEFT JOIN parking_restriction pr_caravan ON pr_caravan.parking_site_id = site.id&#xd;
    AND pr_caravan.type = &apos;CARAVAN&apos;&#xd;
WHERE&#xd;
    site.purpose = &apos;CAR&apos;&#xd;
    AND site.duplicate_of_parking_site_id IS NULL&#xd;
UNION&#xd;
-- the `id` column makes the two sets disjunct anyways, so we allow the optimization&#xd;
ALL&#xd;
SELECT&#xd;
    -- Unique ID for GeoServer (avoids collisions between site and spot ids)&#xd;
    CONCAT(&apos;spot-&apos;, spot.id) AS uid,&#xd;
    spot.id,&#xd;
    spot.parking_site_id,&#xd;
    spot.source_id,&#xd;
    spot.original_uid,&#xd;
    spot.name,&#xd;
    NULL AS operator_name,&#xd;
    spot.purpose :: text :: purposetype,&#xd;
    &apos;spot&apos; AS parking_object,&#xd;
    NULL AS public_url,&#xd;
    NULL AS photo_url,&#xd;
    NULL AS address,&#xd;
    NULL AS opening_hours,&#xd;
    NULL AS description,&#xd;
    NULL AS has_lighting,&#xd;
    NULL AS fee_description,&#xd;
    NULL AS has_fee,&#xd;
    NULL AS park_and_ride_type,&#xd;
    spot.has_realtime_data,&#xd;
    NULL AS realtime_opening_status,&#xd;
    spot.realtime_status,&#xd;
    spot.type :: text AS type,&#xd;
    -- set capacity to 1 for all spots&#xd;
    1 AS capacity,&#xd;
    NULL AS realtime_capacity,&#xd;
    NULL AS realtime_free_capacity,&#xd;
    pr.max_stay AS max_stay,&#xd;
    -- Note: We assume that, for a spot, there is always at most one restriction (of any type).&#xd;
    (&#xd;
        CASE&#xd;
            WHEN pr.type = &apos;DISABLED&apos; THEN 1&#xd;
            ELSE NULL&#xd;
        END&#xd;
    ) AS capacity_disabled,&#xd;
    NULL AS realtime_capacity_disabled,&#xd;
    NULL AS realtime_free_capacity_disabled,&#xd;
    (&#xd;
        CASE&#xd;
            WHEN pr.type = &apos;WOMEN&apos; THEN 1&#xd;
            ELSE NULL&#xd;
        END&#xd;
    ) AS capacity_women,&#xd;
    NULL AS realtime_capacity_women,&#xd;
    NULL AS realtime_free_capacity_women,&#xd;
    (&#xd;
        CASE&#xd;
            WHEN pr.type = &apos;FAMILY&apos; THEN 1&#xd;
            ELSE NULL&#xd;
        END&#xd;
    ) AS capacity_family,&#xd;
    NULL AS realtime_capacity_family,&#xd;
    NULL AS realtime_free_capacity_family,&#xd;
    (&#xd;
        CASE&#xd;
            WHEN pr.type = &apos;CHARGING&apos; THEN 1&#xd;
            ELSE NULL&#xd;
        END&#xd;
    ) AS capacity_charging,&#xd;
    NULL AS realtime_capacity_charging,&#xd;
    NULL AS realtime_free_capacity_charging,&#xd;
    (&#xd;
        CASE&#xd;
            WHEN pr.type = &apos;CARSHARING&apos; THEN 1&#xd;
            ELSE NULL&#xd;
        END&#xd;
    ) AS capacity_carsharing,&#xd;
    NULL AS realtime_capacity_carsharing,&#xd;
    NULL AS realtime_free_capacity_carsharing,&#xd;
    (&#xd;
        CASE&#xd;
            WHEN pr.type = &apos;RESIDENT&apos; THEN 1&#xd;
            ELSE NULL&#xd;
        END&#xd;
    ) AS capacity_resident,&#xd;
    NULL AS realtime_capacity_resident,&#xd;
    NULL AS realtime_free_capacity_resident,&#xd;
    (&#xd;
        CASE&#xd;
            WHEN pr.type = &apos;BUS&apos; THEN 1&#xd;
            ELSE NULL&#xd;
        END&#xd;
    ) AS capacity_bus,&#xd;
    NULL AS realtime_capacity_bus,&#xd;
    NULL AS realtime_free_capacity_bus,&#xd;
    (&#xd;
        CASE&#xd;
            WHEN pr.type = &apos;CARAVAN&apos; THEN 1&#xd;
            ELSE NULL&#xd;
        END&#xd;
    ) AS capacity_caravan,&#xd;
    NULL AS realtime_capacity_caravan,&#xd;
    NULL AS realtime_free_capacity_caravan,&#xd;
    NULL AS max_height,&#xd;
    NULL AS is_covered,&#xd;
    NULL AS supervision_type,&#xd;
    NULL AS related_location,&#xd;
    (&#xd;
        spot.realtime_data_updated_at &lt; now() - interval &apos;30 minutes&apos;&#xd;
    ) AS realtime_data_outdated,&#xd;
    NULL AS side,&#xd;
    NULL AS orientation,&#xd;
    NULL AS parking_type,&#xd;
    spot.created_at,&#xd;
    spot.modified_at,&#xd;
    spot.static_data_updated_at,&#xd;
    spot.realtime_data_updated_at,&#xd;
    GeometryType(ST_SetSRID(ST_GeomFromGeoJSON(geojson), 4326)) AS geojson,&#xd;
    spot.geometry&#xd;
FROM&#xd;
    parking_spot spot&#xd;
    LEFT JOIN parking_restriction pr ON spot.id = pr.parking_spot_id&#xd;
WHERE&#xd;
    spot.purpose = &apos;CAR&apos;
</sql>

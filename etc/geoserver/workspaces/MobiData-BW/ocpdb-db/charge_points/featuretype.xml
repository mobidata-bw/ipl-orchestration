      <virtualTable>
        <name>charge_points</name>
        <sql>-- Sub query &apos;station_power&apos;, which is a list of charging stations&#xd;
-- with their corresponding maximum charging power&#xd;
WITH station_power AS (&#xd;
    SELECT&#xd;
        charging_station_id,&#xd;
        MAX(max_electric_power) AS max_electric_power&#xd;
    FROM&#xd;
        evse&#xd;
        JOIN connector ON evse.id = connector.evse_id&#xd;
    GROUP BY&#xd;
        charging_station_id&#xd;
)&#xd;
SELECT&#xd;
    location.id,&#xd;
    location.lat,&#xd;
    location.lon,&#xd;
    location.source,&#xd;
    location.name,&#xd;
    location.address,&#xd;
    location.postal_code,&#xd;
    location.city,&#xd;
    location.state,&#xd;
    cp.chargepoint_available_count,&#xd;
    cp.chargepoint_unknown_count,&#xd;
    cp.chargepoint_static_count,&#xd;
    cp.chargepoint_charging_count,&#xd;
    cp.chargepoint_inoperative_count,&#xd;
    cp.chargepoint_outoforder_count,&#xd;
    cp.chargepoint_reserved_count,&#xd;
    cp.chargepoint_bike_count,&#xd;
    sp.max_electric_power,&#xd;
    location.geometry,&#xd;
    business.name AS operator_name&#xd;
FROM&#xd;
    charging_station&#xd;
    LEFT JOIN (&#xd;
        SELECT&#xd;
            charging_station_id,&#xd;
            COUNT(es.id) AS chargepoint_count,&#xd;
            SUM(&#xd;
                CASE&#xd;
                    WHEN es.status = &apos;AVAILABLE&apos; THEN 1&#xd;
                    ELSE 0&#xd;
                END&#xd;
            ) AS chargepoint_available_count,&#xd;
            SUM(&#xd;
                CASE&#xd;
                    WHEN es.status = &apos;UNKNOWN&apos; THEN 1&#xd;
                    ELSE 0&#xd;
                END&#xd;
            ) AS chargepoint_unknown_count,&#xd;
            SUM(&#xd;
                CASE&#xd;
                    WHEN es.status = &apos;STATIC&apos; THEN 1&#xd;
                    ELSE 0&#xd;
                END&#xd;
            ) AS chargepoint_static_count,&#xd;
            SUM(&#xd;
                CASE&#xd;
                    WHEN es.status = &apos;CHARGING&apos; THEN 1&#xd;
                    ELSE 0&#xd;
                END&#xd;
            ) AS chargepoint_charging_count,&#xd;
            SUM(&#xd;
                CASE&#xd;
                    WHEN es.status = &apos;INOPERATIVE&apos; THEN 1&#xd;
                    ELSE 0&#xd;
                END&#xd;
            ) AS chargepoint_inoperative_count,&#xd;
            SUM(&#xd;
                CASE&#xd;
                    WHEN es.status = &apos;OUTOFORDER&apos; THEN 1&#xd;
                    ELSE 0&#xd;
                END&#xd;
            ) AS chargepoint_outoforder_count,&#xd;
            SUM(&#xd;
                CASE&#xd;
                    WHEN es.status = &apos;RESERVED&apos; THEN 1&#xd;
                    ELSE 0&#xd;
                END&#xd;
            ) AS chargepoint_reserved_count,&#xd;
            SUM(&#xd;
                CASE&#xd;
                    WHEN es.parking_restrictions &amp; 64 = 64 THEN 1&#xd;
                    ELSE 0&#xd;
                END&#xd;
            ) AS chargepoint_bike_count&#xd;
        FROM&#xd;
            charging_station l&#xd;
            JOIN evse es ON l.id = es.charging_station_id&#xd;
        GROUP BY&#xd;
            charging_station_id&#xd;
    ) AS cp ON cp.charging_station_id = charging_station.id&#xd;
    -- Join results of sub query &apos;station_power&apos; with table &apos;charging_station&apos;&#xd;
    LEFT JOIN station_power sp ON sp.charging_station_id = charging_station.id&#xd;
    -- Join &apos;charging_station&apos; with table &apos;location&apos;&#xd;
    LEFT JOIN location ON charging_station.location_id = location.id&#xd;
     -- Join &apos;location&apos; with table &apos;business&apos;&#xd;
    LEFT JOIN business ON location.operator_id = business.id
</sql>
